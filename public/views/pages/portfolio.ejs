<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
  <title><%= process.env.TITLE %> - portfolio</title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/ultimate.css" />

  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/dynamicUltimateThumbnailManager.js"></script>

</head>

<body onload="getNewUltimateBoard()">

  <% include ../partials/pre-nav.ejs %>
  <% include ../partials/post-nav.ejs %>

  <div class="jumbotron-portfolio">
    <div class="card-portfolio-titlepage card-portrait-portfoliopage">
      <h1 class="bold">Project Portfolio</h1>
    </div>
  </div>

  <div class="content-container">

    <!-- hermes card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-hermes" onclick="window.location='/hermes'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-freelance">freelance</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-false">in progress</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-featured">featured</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2019</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">Hermes</h2>
      </div>
      <div class="card-portfolio-body-container">
          <img class='card-portfolio-image' src='/images/hermes/hermes-black.png'>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">messenger service</div>
        <div class="card-footer-element card-portfolio-footer-element">mongoDB</div>
        <div class="card-footer-element card-portfolio-footer-element">authentication</div>
        <div class="card-footer-element card-portfolio-footer-element">socket.io</div>
      </div>
    </div>

    <!-- notify card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-notify" onclick="window.location='/notify'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-freelance">freelance</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-false">in progress</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-featured">featured</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2020</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">Notify</h2>
      </div>
      <div class="card-portfolio-body-container">
          <img class='card-portfolio-image' src='/images/notify.png'>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">notification service</div>
        <div class="card-footer-element card-portfolio-footer-element">android</div>
        <div class="card-footer-element card-portfolio-footer-element">firebase</div>
        <div class="card-footer-element card-portfolio-footer-element">mongoDB</div>
        <div class="card-footer-element card-portfolio-footer-element">authentication</div>
        <div class="card-footer-element card-portfolio-footer-element">REST API</div>
      </div>
    </div>

    <!-- ultimate card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-ultimate" onclick="window.location='/ultimate'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-freelance">freelance</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-true">completed</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-featured">featured</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2018</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">Ultimate <div style='whitespace:nowrap;'>Tic-Tac-Toe</div></h2>
      </div>
      <div class="card-portfolio-body-container board-ultimate-portfolio" id="dynamic_ultimate">
          <img class='card-portfolio-image' src='/images/ultimate/redO.png'>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">messenger service</div>
        <div class="card-footer-element card-portfolio-footer-element">mongoDB</div>
        <div class="card-footer-element card-portfolio-footer-element">authentication</div>
        <div class="card-footer-element card-portfolio-footer-element">socket.io</div>
      </div>
    </div>

    <!-- anagrams card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-anagrams" onclick="window.location='/anagrams'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-uni">university</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-true">completed</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2017</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">Anagrams</h2>
      </div>
      <div class="small card-portfolio-body-container" style="display:block; scale:0.8;">
        <div>
          <h4 class='no-padding'>Phonetic</h4>
          <div><p>/ˈanəɡram/</p></div>
        </div>
        <div>
          <h4 class='no-padding'>Definitions</h4>
          <div><p><b>noun</b>: A word, phrase, or name formed by rearranging the letters of another, such as spar, formed from rasp.</p></div>
        </div>
        <div>
          <h4 class='no-padding'>Origin</h4>
          <div><p class='no-padding'>Late 16th century from French anagramme or modern Latin anagramma, from Greek ana- ‘back, anew’ + gramma ‘letter’.</p></div>
        </div>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">REST API</div>
        <div class="card-footer-element card-portfolio-footer-element">radix sort</div>
        <div class="card-footer-element card-portfolio-footer-element">socket.io</div>
        <div class="card-footer-element card-portfolio-footer-element">substring search</div>
      </div>
    </div>

    <!-- this website card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-this-website" onclick="window.location='/this-website'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-freelance">freelance</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-false">in progress</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2018</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">This Website</h2>
      </div>
      <div class="card-portfolio-body-container">
          <img class='card-portfolio-image' src='/images/monitor_with_code.jpg'>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">NodeJs</div>
        <div class="card-footer-element card-portfolio-footer-element">express</div>
        <div class="card-footer-element card-portfolio-footer-element">mongoDB</div>
        <div class="card-footer-element card-portfolio-footer-element">socket.io</div>
        <div class="card-footer-element card-portfolio-footer-element">HTML</div>
        <div class="card-footer-element card-portfolio-footer-element">CSS</div>
        <div class="card-footer-element card-portfolio-footer-element">authentication</div>
        <div class="card-footer-element card-portfolio-footer-element">REST API</div>
      </div>
    </div>

    <!-- bwt card -->
    <div class="card bordered shadowed card-portfolio pointer-mouse card-portfolio-bwt" onclick="window.location='/bwt'">
      <div class="card-header">
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-type card-portfolio-header-element-type-uni">university</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-completed card-portfolio-header-element-completed-true">completed</div>
        <div class="card-header-element card-portfolio-header-element card-portfolio-header-element-year">2017</div>
      </div>
      <div class="card-portfolio-title-container">
        <h2 class="card-title card-portfolio-title bold">Burrows-Wheeler Transform</h2>
      </div>
      <div class="card-portfolio-body-container">
          <img class='card-portfolio-image' src='/images/BWT-mississippi.png'>
      </div>
      <div class="card-footer card-portfolio-footer">
        <div class="card-footer-element card-portfolio-footer-element">compression algorithm</div>
        <div class="card-footer-element card-portfolio-footer-element">run-length encoding</div>
        <div class="card-footer-element card-portfolio-footer-element">socket.io</div>
      </div>
    </div>

</body>
<script>getNewUltimateBoard()</script>
</html>
